{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "artbyjulieulfeng.vercel.app"
        }
      ],
      "destination": "/index.html"
    }
  ],
  "ignoreFiles": [],
  "routes": [
    {
      "src": "/(robots.txt|sitemap.xml)",
      "dest": "/$1"
    }
  ]
}